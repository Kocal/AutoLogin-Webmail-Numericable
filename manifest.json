{
  "manifest_version": 2,
  "name": "AutoLogin WebMail Numericable",
  "description": "Extension permettant de se connecter automatiquement à la webmail de Numéricable.",
  "version": "1.1",
  "permissions": [
    "tabs",
    "storage",
    "https://webmail.numericable.fr/"
  ],
  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32x32.png",
    "64": "icons/64x64.png",
    "128": "icons/128x128.png"
  },
  "options_page": "options.html",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_title": "AutoLogin WebMail Numericable"
  }
}
