{
   "background": {
      "scripts": [ "js/background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "38": "icons/38x38.png"
      },
      "default_title": "AutoLogin WebMail Numericable"
   },
   "description": "Extension qui permet de se connecter automatiquement à la webmail de Numéricable",
   "icons": {
      "16": "icons/16x16.png",
      "48": "icons/48x48.png"
   },
   "manifest_version": 2,
   "name": "AutoLogin WebMail Numericable",
   "options_page": "options.html",
   "permissions": [ "tabs", "storage", "https://webmail.numericable.fr/" ],
   "version": "1.1"
}
